    "eslint": "^5.5.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.2",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.1",
    "typescript": "~2.9.2",
    "webpack": "^4.17.2",
    "webpack-dev-server": "^3.1.7"
  },
  "jupyterlab": {
    "extension": true
